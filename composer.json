{
    "name": "arokettu/phpstorm-metadata-export",
    "description": "Export PhpStorm Advanced Metadata from DI containers",
    "keywords": [
        "dependency injection", "phpstorm", "code completion",
        "pimple", "php-di", "laminas servicemanager",
        "slim", "psr-15",
        "dev"
    ],
    "homepage": "https://sandfox.dev/php/metadata-exporter-phpstorm.html",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Smirnov",
            "email": "sandfox@sandfox.me",
            "role": "developer",
            "homepage": "https://sandfox.me/"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/sandfox/phpstorm-metadata-export/-/issues",
        "source": "https://gitlab.com/sandfox/phpstorm-metadata-export",
        "docs": "https://phpstorm-metadata-export.readthedocs.io/",
        "chat": "https://gitter.im/arokettu/community"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\PhpStorm\\Metadata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\PhpStorm\\Metadata\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2 | ^8.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-servicemanager": "^3.0",
        "php-di/php-di": "^6.0 | ^7.0",
        "phpunit/phpunit": ">= 7 < 10",
        "pimple/pimple": "^3.0",
        "psy/psysh": "*",
        "sandfox.dev/code-standard": "^1",
        "slim/slim": "^3.0",
        "squizlabs/php_codesniffer": "*",
        "symfony/polyfill-php80": "^1.23",
        "vimeo/psalm": "^4.30 | ^5.4"
    }
}
